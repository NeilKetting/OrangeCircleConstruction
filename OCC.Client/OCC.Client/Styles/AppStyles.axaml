<Styles xmlns="https://github.com/avaloniaui"
		xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">


	<Design.PreviewWith>
		<StackPanel Width="100">
			<Button Classes="SidebarButton" Content="Test 1" />
		</StackPanel>
	</Design.PreviewWith>

	<!-- ========================================================================= -->
	<!-- TEXT STYLES -->
	<!-- ========================================================================= -->

	<Style Selector="TextBlock.BrandName">
		<Setter Property="Foreground" Value="White" />
		<Setter Property="FontSize" Value="{DynamicResource FontSizeLarge}" />
		<Setter Property="FontWeight" Value="Bold" />
		<Setter Property="VerticalAlignment" Value="Center" />
	</Style>

	<Style Selector="TextBlock.H1">
		<Setter Property="FontSize" Value="{DynamicResource FontSizeXL}"/>
		<Setter Property="FontWeight" Value="SemiBold"/>
		<Setter Property="Foreground" Value="{DynamicResource TextPrimaryDark}"/>
	</Style>

	<Style Selector="TextBlock.LabelXSM">
		<Setter Property="FontSize" Value="{DynamicResource FontSizeXS}"/>
		<Setter Property="FontWeight" Value="Bold"/>
		<Setter Property="Foreground" Value="{DynamicResource TextTertiarySlate}"/>
	</Style>

	<Style Selector="TextBlock.LabelSmall">
		<Setter Property="FontSize" Value="{DynamicResource FontSizeSmall}"/>
		<Setter Property="FontWeight" Value="Bold"/>
		<Setter Property="Foreground" Value="{DynamicResource TextTertiarySlate}"/>
	</Style>

	<Style Selector="TextBlock.FormLabel">
		<Setter Property="FontSize" Value="{DynamicResource FontSizeSmall}"/>
		<Setter Property="Foreground" Value="{DynamicResource TextSecondaryDark}"/>
		<Setter Property="FontWeight" Value="SemiBold"/>
	</Style>

	<Style Selector="TextBlock.StatLabel">
		<Setter Property="FontSize" Value="{DynamicResource FontSizeXS}"/>
		<Setter Property="FontWeight" Value="Bold"/>
		<Setter Property="Foreground" Value="{DynamicResource TextSecondaryGray}"/>
		<Setter Property="Margin" Value="0,0,0,5"/>
	</Style>

	<Style Selector="TextBlock.StatValue">
		<Setter Property="FontSize" Value="{DynamicResource FontSizeLarge}"/>
		<Setter Property="FontWeight" Value="Medium"/>
		<Setter Property="Foreground" Value="{DynamicResource TextPrimaryDark}"/>
	</Style>

	<!-- ========================================================================= -->
	<!-- BUTTON STYLES -->
	<!-- ========================================================================= -->

	<!-- Base Button -->
	<Style Selector="Button">
		<Setter Property="Cursor" Value="Hand"/>
	</Style>

	<!-- Shared Button Transitions (Background) -->
	<Style Selector="Button.Primary /template/ ContentPresenter,
	Button.Standout /template/ ContentPresenter,
	Button.SidebarButton /template/ ContentPresenter,
	Button.TabButton /template/ ContentPresenter,
	Button.Ghost /template/ ContentPresenter,
	Button.Secondary /template/ ContentPresenter,
	Button.IconButton /template/ ContentPresenter">
		<Setter Property="Transitions">
			<Transitions>
				<BrushTransition Property="Background" Duration="0:0:0.15"/>
			</Transitions>
		</Setter>
	</Style>

	<!-- Shared Action Button Styles (Primary, Cancel & Standout) -->
	<Style Selector="Button.Primary, Button.Standout, Button.Cancel">
		<Setter Property="Height" Value="40" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="HorizontalContentAlignment" Value="Center" />
		<Setter Property="VerticalContentAlignment" Value="Center" />
		<Setter Property="FontWeight" Value="SemiBold" />
		<Setter Property="CornerRadius" Value="4" />
		<Setter Property="Padding" Value="20,0"/>
	</Style>

	<!-- Primary Action Button (Teal) -->
	<Style Selector="Button.Primary">
		<Setter Property="Background" Value="{DynamicResource PrimaryTeal}" />
		<Setter Property="BorderBrush" Value="{DynamicResource SurfaceWhite}"/>
		<Setter Property="BorderThickness" Value="0.5"/>
	</Style>
	<Style Selector="Button.Primary:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource PrimaryTealHover}" />
	</Style>
	<Style Selector="Button.Primary:pressed /template/ ContentPresenter">
		<Setter Property="Opacity" Value="0.8"/>
		<Setter Property="RenderTransform" Value="scale(0.98)"/>
	</Style>

	<!-- Secondary Button (Orange) -->
	<Style Selector="Button.Secondary">
		<Setter Property="Background" Value="{DynamicResource AccentOrange}" />
		<Setter Property="Foreground" Value="{DynamicResource TextPrimaryDark}"/>
	</Style>
	<Style Selector="Button.Primary:pressed /template/ ContentPresenter">
		<Setter Property="Opacity" Value="0.8"/>
		<Setter Property="RenderTransform" Value="scale(0.98)"/>
	</Style>

	<!-- Cancel Button (Gray) -->
	<Style Selector="Button.Cancel">
		<Setter Property="Background" Value="{DynamicResource CancelGrayButtonBackground}" />
		<Setter Property="Foreground" Value="{DynamicResource TextPrimaryDark}"/>
	</Style>
	<Style Selector="Button.Primary:pressed /template/ ContentPresenter">
		<Setter Property="Opacity" Value="0.8"/>
		<Setter Property="RenderTransform" Value="scale(0.98)"/>
	</Style>


	<!-- Standout Green Action Button (Green) -->
	<Style Selector="Button.Standout">
		<Setter Property="Background" Value="{DynamicResource GreenButtonBackground}" />
	</Style>

	<!-- Ghost Button -->
	<Style Selector="Button.Ghost">
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="BorderThickness" Value="0"/>
		<Setter Property="Padding" Value="10,5"/>
	</Style>
	<Style Selector="Button.Ghost:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="#10000000"/>
	</Style>

	<!-- Close Button Style (Red Hover) -->
	<Style Selector="Button.Close">
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="BorderThickness" Value="0"/>
		<Setter Property="Padding" Value="5"/>
		<Setter Property="Foreground" Value="{DynamicResource TextSecondaryGray}"/>
	</Style>
	<Style Selector="Button.Close:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="Foreground" Value="#EF4444"/>
	</Style>
	<Style Selector="Button.Close:pointerover PathIcon">
		<Setter Property="Foreground" Value="#EF4444"/>
	</Style>

	<!-- Icon Button (Transparent, usually for toolbars) -->
	<Style Selector="Button.IconButton">
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="FontSize" Value="{DynamicResource FontSizeSmall}"/>
		<Setter Property="BorderThickness" Value="0"/>
		<Setter Property="Padding" Value="0"/>
		<Setter Property="Cursor" Value="Hand" />
	</Style>
	<Style Selector="Button.IconButton:pointerover /template/ ContentPresenter">
		<Setter Property="Cursor" Value="Hand" />
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="Opacity" Value="0.7"/>
	</Style>

	<!-- Icon Button Close (Extends IconButton but adds Red Hover) -->
	<Style Selector="Button.IconButtonClose">
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="FontSize" Value="{DynamicResource FontSizeSmall}"/>
		<Setter Property="BorderThickness" Value="0"/>
		<Setter Property="Padding" Value="0"/>
		<Setter Property="Cursor" Value="Hand" />
	</Style>
	<Style Selector="Button.IconButtonClose:pointerover /template/ ContentPresenter">
		<Setter Property="Cursor" Value="Hand" />
		<Setter Property="Background" Value="Transparent"/>
	</Style>
	<Style Selector="Button.IconButtonClose:pointerover PathIcon">
		<Setter Property="Foreground" Value="#EF4444"/>
	</Style>

	<!-- Social Button (Auth) -->
	<Style Selector="Button.Social">
		<Setter Property="Background" Value="{DynamicResource AppBackgroundSlate}" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="Height" Value="45" />
		<Setter Property="CornerRadius" Value="4" />
		<Setter Property="BorderThickness" Value="0" />
	</Style>

	<!-- Sidebar Button -->
	<Style Selector="Button.SidebarButton">
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="Foreground" Value="{DynamicResource TextPrimaryDark}"/>
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
		<Setter Property="HorizontalContentAlignment" Value="Left"/>
		<Setter Property="Height" Value="40"/>
		<Setter Property="ClipToBounds" Value="False"/>
		<Setter Property="Padding" Value="30,0,0,0"/>
	</Style>
	<Style Selector="Button.SidebarButton:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource AppBackgroundSlate}"/>
	</Style>
	<Style Selector="Button.SidebarButton:pressed /template/ ContentPresenter">
		<Setter Property="Opacity" Value="0.8"/>
		<Setter Property="RenderTransform" Value="scale(0.98)"/>
	</Style>
	<Style Selector="Button.SidebarButton.Active">
		<Setter Property="Background" Value="{DynamicResource AppBackgroundSlate}"/>
		<Setter Property="BorderThickness" Value="4,0,0,0"/>
	</Style>

	<Style Selector="Button.SidebarButton.Collapsed">
		<Setter Property="HorizontalContentAlignment" Value="Center"/>
		<Setter Property="Padding" Value="0"/>
		<Setter Property="VerticalContentAlignment" Value="Center"/>
	</Style>

	<!-- Popup Menu Button (For Dropdowns) -->
	<Style Selector="Button.PopupMenuButton">
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="Foreground" Value="{DynamicResource TextPrimaryDark}"/>
		<Setter Property="HorizontalAlignment" Value="Stretch"/>
		<Setter Property="HorizontalContentAlignment" Value="Left"/>
		<Setter Property="Height" Value="40"/>
		<Setter Property="Padding" Value="15,0"/>
		<Setter Property="CornerRadius" Value="4"/>
		<Setter Property="Margin" Value="5,0"/>
	</Style>
	<Style Selector="Button.PopupMenuButton:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource SurfaceLightGray}"/>
	</Style>
	<Style Selector="Button.PopupMenuButton:pressed /template/ ContentPresenter">
		<Setter Property="Opacity" Value="0.8"/>
		<Setter Property="RenderTransform" Value="scale(0.98)"/>
	</Style>

	<!-- Tab Buttons -->
	<Style Selector="Button.TabButton">
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="Foreground" Value="{DynamicResource TextPrimaryDark}"/>
		<Setter Property="FontSize" Value="{DynamicResource FontSizeSmall}"/>
		<Setter Property="FontWeight" Value="SemiBold"/>
		<Setter Property="CornerRadius" Value="0"/>
		<Setter Property="Padding" Value="20,0"/>
		<Setter Property="Height" Value="62"/>
		<Setter Property="Margin" Value="-1"/>

		<Setter Property="VerticalContentAlignment" Value="Center"/>
	</Style>
	<Style Selector="Button.TabButton:pressed /template/ ContentPresenter">
		<Setter Property="Opacity" Value="0.8"/>
		<Setter Property="RenderTransform" Value="scale(0.98)"/>
	</Style>
	<Style Selector="Button.TabButton:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource SurfaceLightGray}"/>
	</Style>
	<Style Selector="Button.TabButton.Active /template/ ContentPresenter">
		<Setter Property="Background" Value="{DynamicResource SurfaceLightGray}"/>
	</Style>


	<!-- ========================================================================= -->
	<!-- INPUT STYLES -->
	<!-- ========================================================================= -->

	<!-- Shared Input Styles (TextBox, ComboBox, DatePicker) -->
	<Style Selector="TextBox, ComboBox, DatePicker">
		<Setter Property="Height" Value="36"/>
		<Setter Property="BorderThickness" Value="1" />
		<Setter Property="BorderBrush" Value="{DynamicResource BorderGray}" />
		<Setter Property="Background" Value="{DynamicResource SurfaceWhite}" />
		<Setter Property="Foreground" Value="{DynamicResource TextPrimaryDark}" />
		<Setter Property="CornerRadius" Value="4" />
		<Setter Property="HorizontalAlignment" Value="Stretch" />
		<Setter Property="VerticalAlignment" Value="Center" />
	</Style>

	<!-- Validation Error Style (Official Doc Pattern) -->
	<Style Selector="DataValidationErrors">
		<Setter Property="Template">
			<ControlTemplate>
				<DockPanel LastChildFill="True">
					<ContentControl DockPanel.Dock="Right"
									ContentTemplate="{TemplateBinding ErrorTemplate}"
									DataContext="{TemplateBinding Owner}"
									Content="{Binding (DataValidationErrors.Errors)}"
									IsVisible="{Binding (DataValidationErrors.HasErrors)}"/>
					<ContentPresenter Name="PART_ContentPresenter"
									  Background="{TemplateBinding Background}"
									  BorderBrush="{TemplateBinding BorderBrush}"
									  BorderThickness="{TemplateBinding BorderThickness}"
									  CornerRadius="{TemplateBinding CornerRadius}"
									  ContentTemplate="{TemplateBinding ContentTemplate}"
									  Content="{TemplateBinding Content}"
									  Padding="{TemplateBinding Padding}"/>
				</DockPanel>
			</ControlTemplate>
		</Setter>
		<Setter Property="ErrorTemplate">
			<DataTemplate>
				<Canvas Width="14" Height="14" Margin="4,0,1,0" Background="Transparent">
					<Canvas.Styles>
						<Style Selector="ToolTip">
							<Setter Property="Background" Value="#CCFF0000"/>
							<Setter Property="BorderBrush" Value="Red"/>
							<Setter Property="Foreground" Value="White"/>
						</Style>
					</Canvas.Styles>
					<ToolTip.Tip>
						<ItemsControl ItemsSource="{Binding}"/>
					</ToolTip.Tip>
					<Path Data="M14,7 A7,7 0 0,0 0,7 M0,7 A7,7 0 1,0 14,7 M7,3l0,5 M7,9l0,2" 
						  Stroke="Red" 
						  StrokeThickness="2"/>
				</Canvas>
			</DataTemplate>
		</Setter>
	</Style>

	<!-- Generic Error State (Proven to work) -->
	<Style Selector=":is(Control):error /template/ Border">
		<Setter Property="BorderBrush" Value="Red"/>
		<Setter Property="BorderThickness" Value="2"/>
	</Style>

	<!-- Specific Input Overrides -->
	<Style Selector="TextBox">
		<Setter Property="Padding" Value="12,6" />
		<Setter Property="CaretBrush" Value="{DynamicResource TextPrimaryDark}" />
	</Style>

	<Style Selector="ComboBox">
		<Setter Property="Padding" Value="12,6" />
	</Style>

	<Style Selector="DatePicker">
		<Setter Property="Padding" Value="8,8" />
	</Style>

	<!-- Fix for unreadable dark gray hover/focus on TextBoxes -->
	<Style Selector="TextBox:pointerover /template/ Border#PART_BorderElement">
		<Setter Property="BorderBrush" Value="{DynamicResource PrimaryTeal}" />
		<Setter Property="Background" Value="{DynamicResource SurfaceWhite}" />
	</Style>
	<Style Selector="TextBox:focus /template/ Border#PART_BorderElement">
		<Setter Property="BorderBrush" Value="{DynamicResource PrimaryTeal}" />
		<Setter Property="BorderThickness" Value="2" />
		<Setter Property="Background" Value="{DynamicResource SurfaceWhite}" />
	</Style>

	<!-- Global Watermark/Placeholder Fix -->
	<Style Selector="TextBox /template/ TextBlock#PART_Watermark, ComboBox /template/ TextBlock#PART_PlaceholderText">
		<Setter Property="Foreground" Value="{DynamicResource TextTertiarySlate}" />
	</Style>

	<!-- ========================================================================= -->
	<!-- CONTAINER & BORDER STYLES -->
	<!-- ========================================================================= -->

	<Style Selector="Border.LogoContainer">
		<Setter Property="Width" Value="140" />
		<Setter Property="Height" Value="140" />
		<Setter Property="CornerRadius" Value="80" />
		<Setter Property="Background" Value="White" />
		<Setter Property="Padding" Value="5" />
	</Style>

	<Style Selector="Border.TopHeader">
		<Setter Property="Background" Value="{DynamicResource SurfaceLightGray}"/>
		<Setter Property="Padding" Value="20"/>
	</Style>

	<Style Selector="StackPanel.SidebarItem">
		<Setter Property="Height" Value="45"/>
		<Setter Property="Spacing" Value="15"/>
		<Setter Property="Margin" Value="0,2"/>
		<Setter Property="ClipToBounds" Value="False"/>
	</Style>

	<Style Selector="Border.TopBar">
		<Setter Property="Background" Value="{DynamicResource SurfaceWhite}"/>
		<Setter Property="Margin" Value="0"/>
		<Setter Property="Height" Value="60"/>
	</Style>

	<Style Selector="Border.Card">
		<Setter Property="Background" Value="{DynamicResource SurfaceWhite}" />
		<Setter Property="Padding" Value="40" />
		<Setter Property="CornerRadius" Value="8" />
		<Setter Property="BoxShadow" Value="0 10 30 0 #20000000" />
	</Style>

	<Style Selector="Border.WidgetCard">
		<Setter Property="Background" Value="{DynamicResource SurfaceWhite}"/>
		<Setter Property="CornerRadius" Value="8"/>
		<Setter Property="Padding" Value="20"/>
		<Setter Property="Margin" Value="10"/>
		<Setter Property="BoxShadow" Value="0 1 3 0 #10000000"/>
	</Style>

	<Style Selector="StackPanel.Stat">
		<Setter Property="Margin" Value="0,0,40,0"/>
	</Style>

	<!-- ========================================================================= -->
	<!-- DATAGRID STYLES -->
	<!-- DataGrid GLOBAL STYLE!!!!!!!!! -->
	<!-- ========================================================================= -->

	<Style Selector="DataGridColumnHeader">
		<Setter Property="Foreground" Value="{DynamicResource TextSecondaryGray}"/>
		<Setter Property="FontWeight" Value="SemiBold"/>
		<Setter Property="FontSize" Value="{DynamicResource FontSizeMedium}"/>
		<Setter Property="Background" Value="#F8FAFC"/>
		<Setter Property="Padding" Value="12,10"/>
	</Style>
	<Style Selector="DataGridRow">
		<Setter Property="Height" Value="40"/>
		<Setter Property="Background" Value="White"/>
		<Setter Property="Padding" Value="0,5"/>
	</Style>

	<Style Selector="DataGridCell">
		<Setter Property="Padding" Value="10,4"/>
		<Setter Property="VerticalContentAlignment" Value="Center"/>
		<Setter Property="MinHeight" Value="35"/>
		<Setter Property="FontSize" Value="{DynamicResource FontSizeSmall}"/>
	</Style>

	<!-- Remove default focus/selected border from cells - AGGRESSIVE REMOVAL -->
	<Style Selector="DataGridCell">
		<Setter Property="FocusAdorner" Value="{x:Null}" />
		<Setter Property="BorderThickness" Value="0"/>
		<Setter Property="BorderBrush" Value="Transparent"/>
	</Style>
	<Style Selector="DataGridCell:current">
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="0"/>
	</Style>
	<Style Selector="DataGridCell:focus">
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="0"/>
	</Style>
	<Style Selector="DataGridCell:selected">
		<Setter Property="BorderBrush" Value="Transparent" />
		<Setter Property="BorderThickness" Value="0"/>
	</Style>

	<!-- Hide the specific focus/currency visual rectangle -->
	<Style Selector="DataGridCell /template/ Rectangle#CurrencyVisual">
		<Setter Property="IsVisible" Value="False" />
	</Style>
	<Style Selector="DataGridCell:current /template/ Rectangle#CurrencyVisual">
		<Setter Property="IsVisible" Value="False" />
	</Style>
	<Style Selector="DataGridCell:focus /template/ Rectangle#CurrencyVisual">
		<Setter Property="IsVisible" Value="False" />
	</Style>
	<Style Selector="DataGridCell:selected /template/ Rectangle#CurrencyVisual">
		<Setter Property="IsVisible" Value="False" />
	</Style>
	<Style Selector="DataGridCell:focus /template/ Grid#FocusVisual">
		<Setter Property="IsVisible" Value="False" />
	</Style>

	<Style Selector="DataGridRow:selected /template/ Rectangle#BackgroundRectangle">
		<Setter Property="Fill" Value="{DynamicResource HomeMenuGradient}"/>
		<Setter Property="Opacity" Value="0.5"/>
	</Style>

	<!-- ========================================================================= -->
	<!-- MENU & FLYOUT STYLES -->
	<!-- ========================================================================= -->

	<Style Selector="FlyoutPresenter.SidebarFlyout">
		<Setter Property="Padding" Value="15"/>
		<Setter Property="MinWidth" Value="320"/>
		<Setter Property="Background" Value="Transparent"/>
		<Setter Property="BorderThickness" Value="0"/>
		<Setter Property="CornerRadius" Value="6"/>
		<Setter Property="ClipToBounds" Value="False"/>
	</Style>

	<Style Selector="FlyoutPresenter.SidebarFlyout /template/ Border#PART_BorderElement">
		<Setter Property="BoxShadow" Value="0 10 50 0 #45000000"/>
		<Setter Property="Background" Value="White"/>
		<Setter Property="BorderBrush" Value="#94A3B8"/>
		<Setter Property="BorderThickness" Value="1"/>
		<Setter Property="CornerRadius" Value="6"/>
		<Setter Property="ClipToBounds" Value="False"/>
	</Style>

	<Style Selector="MenuItem.ProfileHeader">
		<Setter Property="VerticalAlignment" Value="Center"/>
		<Setter Property="Template">
			<ControlTemplate>
				<TextBlock Text="{Binding Header, RelativeSource={RelativeSource TemplatedParent}}"
						   FontSize="{DynamicResource FontSizeXS}"
						   FontWeight="Bold"
						   Foreground="#94A3B8"
						   Padding="16,14,16,10"/>
			</ControlTemplate>
		</Setter>
	</Style>

	<Style Selector="MenuItem.ProfileItem">
		<Setter Property="Height" Value="48"/>
		<Setter Property="Padding" Value="16,0"/>
		<Setter Property="Foreground" Value="#1E293B"/>
		<Setter Property="FontSize" Value="{DynamicResource FontSizeLarge}"/>
	</Style>
	<Style Selector="MenuItem.ProfileItem /template/ ContentPresenter#PART_HeaderPresenter">
		<Setter Property="Margin" Value="0"/>
	</Style>
	<Style Selector="MenuItem.ProfileItem:pointerover /template/ ContentPresenter#PART_HeaderPresenter">
		<Setter Property="Background" Value="#F8FAFC"/>
	</Style>
	<Style Selector="MenuItem.ProfileItem:pointerover /template/ ContentPresenter">
		<Setter Property="Background" Value="#F8FAFC"/>
	</Style>

	<Style Selector="Separator.MenuSeparator">
		<Setter Property="Background" Value="#E2E8F0"/>
		<Setter Property="Height" Value="1"/>
		<Setter Property="Margin" Value="0"/>
	</Style>

	<!-- ========================================================================= -->
	<!-- ICON STYLES & RESOURCES -->
	<!-- ========================================================================= -->

	<Style Selector="PathIcon.IconStyle">
		<Setter Property="Foreground" Value="{DynamicResource TextSecondaryDark}" />
	</Style>

